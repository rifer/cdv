<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="historical" path="/">
        <default key="_controller">AppBundle:Historical:index</default>
    </route>

    <route id="historical_show" path="/{id}/show">
        <default key="_controller">AppBundle:Historical:show</default>
    </route>

    <route id="historical_new" path="/new">
        <default key="_controller">AppBundle:Historical:new</default>
    </route>

    <route id="historical_create" path="/create" methods="POST">
        <default key="_controller">AppBundle:Historical:create</default>
    </route>

    <route id="historical_edit" path="/{id}/edit">
        <default key="_controller">AppBundle:Historical:edit</default>
    </route>

    <route id="historical_update" path="/{id}/update" methods="POST|PUT">
        <default key="_controller">AppBundle:Historical:update</default>
    </route>

    <route id="historical_delete" path="/{id}/delete" methods="POST|DELETE">
        <default key="_controller">AppBundle:Historical:delete</default>
    </route>

    <route id="historical_edition" path="/{id}/{object_class}/edition">
        <default key="_controller">AppBundle:Woman:edition</default>
    </route>

</routes>
